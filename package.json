{
  "name": "ng-graphql-github",
  "version": "1.0.0",
  "description": "Demo application for showing GraphQL with Angular",
  "main": "src/index.html",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot --prod --progress false --base-href \"https://crevil.github.io/ng-graphql-github/\"",
    "postbuild": "cp dist/index.html dist/404.html",
    "test": "ng test",
    "test.single": "npm run test -- --single-run --code-coverage --progress false --reporters dots",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e --aot --prod --progress false",
    "verify": "npm run lint && npm run test.single && npm run e2e"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Crevil/ng-graphql-github.git"
  },
  "keywords": [
    "angular",
    "graphql",
    "apollo"
  ],
  "author": "Crevil <https://github.com/Crevil>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Crevil/ng-graphql-github/issues"
  },
  "homepage": "https://github.com/Crevil/ng-graphql-github#readme",
  "dependencies": {
    "@angular/animations": "4.1.2",
    "@angular/common": "4.1.2",
    "@angular/compiler": "4.1.2",
    "@angular/core": "4.1.2",
    "@angular/forms": "4.1.2",
    "@angular/http": "4.1.2",
    "@angular/material": "2.0.0-beta.5",
    "@angular/platform-browser": "4.1.2",
    "@angular/platform-browser-dynamic": "4.1.2",
    "@angular/router": "4.1.2",
    "core-js": "2.4.1",
    "rxjs": "5.4.0",
    "uuid": "3.0.1",
    "zone.js": "0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "4.1.2",
    "@types/jasmine": "2.5.45",
    "@types/node": "6.0.73",
    "@types/uuid": "2.0.29",
    "codelyzer": "3.0.1",
    "jasmine-core": "2.6.1",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.1.2",
    "ts-node": "3.0.4",
    "tslint": "5.2.0",
    "typescript": "2.3.2"
  }
}
